s3cmd sync . s3://www.arbryant101.com/ -P --exclude='*' --include='index.htm' --include='style.css'
s3cmd sync gfx s3://www.arbryant101.com/ -P --exclude='*' --include='*.jpg'
s3cmd sync lib s3://www.arbryant101.com/ -P --exclude='*' --include='*.js'
s3cmd sync fonts s3://www.arbryant101.com/ -P --exclude='*' --include='*.svg' --include='*.eot' --include='*.ttf' --include='*.woff'
s3cmd sync prbryant s3://www.arbryant101.com/ -P --exclude='*.swp'
